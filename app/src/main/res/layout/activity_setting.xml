<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".profile.SettingActivity"
    android:orientation="vertical"
    android:background="#e4e5e9">

    <Button
        android:id="@+id/btdx"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:background="#457ad6"
        android:text="Log out"
        android:textColor="#fff"
        android:textSize="20dp"
        android:textStyle="bold"
        android:layout_marginTop="30dp"
        android:layout_gravity="center"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20dp"
        android:text="Support"
        android:padding="5dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:drawableLeft="@drawable/chat"
        android:drawableRight="@drawable/tich"
        android:drawablePadding="50dp"
        android:padding="10dp"
        android:paddingLeft="5dp"
        android:layout_gravity="center"
        android:text="Chat Support"
        android:textSize="18dp"
        android:background="#fff"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:drawableLeft="@drawable/support"
        android:drawableRight="@drawable/tich"
        android:drawablePadding="50dp"
        android:padding="10dp"
        android:paddingLeft="5dp"
        android:layout_gravity="center"
        android:text="Whatsapp Support"
        android:textSize="18dp"
        android:background="#fff"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20dp"
        android:text="More"
        android:padding="5dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:drawableLeft="@drawable/about"
        android:drawableRight="@drawable/tich"
        android:drawablePadding="50dp"
        android:padding="10dp"
        android:paddingLeft="5dp"
        android:layout_gravity="center"
        android:text="About Us"
        android:textSize="18dp"
        android:background="#fff"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:drawableLeft="@drawable/police"
        android:drawableRight="@drawable/tich"
        android:drawablePadding="50dp"
        android:padding="10dp"
        android:paddingLeft="5dp"
        android:layout_gravity="center"
        android:text="Policies"
        android:textSize="18dp"
        android:background="#fff"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:drawableLeft="@drawable/share"
        android:drawableRight="@drawable/tich"
        android:drawablePadding="50dp"
        android:padding="10dp"
        android:paddingLeft="5dp"
        android:layout_gravity="center"
        android:text="Share our App"
        android:textSize="18dp"
        android:background="#fff"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:drawableLeft="@drawable/tera"
        android:drawableRight="@drawable/tich"
        android:drawablePadding="50dp"
        android:padding="10dp"
        android:paddingLeft="5dp"
        android:layout_gravity="center"
        android:text="Rate Us"
        android:textSize="18dp"
        android:background="#fff"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="18dp"
        android:text="App Version 3.1.13 (113)"
        android:padding="5dp"
        android:layout_gravity="center"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"/>






</LinearLayout>